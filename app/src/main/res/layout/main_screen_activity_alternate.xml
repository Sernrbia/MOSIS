<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/main_screen_alternate_notifications"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notifications"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="405dp" />

    <Button
        android:id="@+id/main_screen_alternate_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="341dp" />

    <Button
        android:id="@+id/main_screen_alternate_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="253dp" />

    <Button
        android:id="@+id/main_screen_alternate_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Map"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="170dp" />

    <Button
        android:id="@+id/main_screen_alternate_rank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
